<?xml version="1.0" encoding="iso-8859-1"?>

<settings>
	<data_type>csv</data_type>
	<thousands_separator>none</thousands_separator>
	<depth>15</depth>
	<angle>30</angle>

	<column>
		<width>55</width>
		<spacing>0</spacing>
		<grow_time>1</grow_time>
		<grow_effect>strong</grow_effect>
	</column>
	<plot_area>
		<margins>
		  <left>40</left>
		  <right>20</right>
		</margins>
	  </plot_area>
	<axes>
		<category>
		  <width>1</width>
		  <color>E7E7E7</color>
		</category>
		<value>
		  <width>1</width>
		  <color>E7E7E7</color>
		</value>
	  </axes> 
	<grid>
		<category>
			<alpha>10</alpha>
			<dashed>true</dashed>
		</category>

		<value>
			<alpha>10</alpha>
			<dashed>true</dashed>
		</value>
	</grid>
	<values>
		<category>
		  <color>999999</color>
		  <text_size>10</text_size>
		</category>
		<value>
		  <color>999999</color>
		  <text_size>10</text_size>
		  <min>0</min>
		</value>
	</values>
	<balloon>
		<text_color>000000</text_color>
		<corner_radius>4</corner_radius>
		<border_width>3</border_width>
		<border_alpha>50</border_alpha>
		<border_color>#000000</border_color>
		<alpha>80</alpha>
	</balloon>
	<labels>
		<label lid="0">
			  <x>-5</x>
			  <y>200</y>
			  <width>0</width>
			  <text_color>333333</text_color>
			  <text_size>15</text_size>
			  <rotate>1</rotate>
			  <align>center</align>
		</label>
	</labels>
	<legend>
		<enabled/>
		<align>center</align>
		<border_alpha>100</border_alpha>
		<border_color>E7E7E7</border_color>
		<text_size>12</text_size>
		<margins>5</margins>
	</legend>
	<strings>
    	<no_data>No Data for selected Geography/Period</no_data>
    </strings>
</settings>