<input class="ac_input" autocomplete="off" id="lookup_village" type="text">
<a href="#" id="del_village">
<img src="/media/img/admin/icon_deletelink.gif">
</a>
<script type="text/javascript">
            if ($('#lookup_village').val()) {
                $('#del_village').show()
            }
            $('#lookup_village').autocomplete('../search/', {
                extraParams: {
                    search_fields: 'village_name',
                    app_label: 'dashboard',
                    model_name: 'village',
                },
            }).result(function(event, data, formatted) {
                if (data) {
                    $('#id_village').val(data[1]);
                    $('#del_village').show();
		    filter();
                }
            });
            $('#del_village').click(function(ele, event) {
                $('#id_village').val('');
                $('#del_village').hide();
                $('#lookup_village').val('');
            });
            </script>
        <a href="/admin/dashboard/village/add/" class="add-another" id="add_id_village" onclick="return showAddAnotherPopup(this);"> <img src="/media/img/admin/icon_addlink.gif" alt="Add Another" height="10" width="10"></a>
          
          
