<settings>
  <redraw>1</redraw>
  <data_type>csv</data_type>
  <skip_rows>1</skip_rows>
  <background>
    <alpha>100</alpha>
    <border_color></border_color>
    <border_alpha></border_alpha>
  </background>
  <plot_area>
    <border_color></border_color>
  </plot_area>
  <grid>
    <x>
      <color>006600</color>
      <alpha>5</alpha>
    </x>
    <y>
      <color>006600</color>
      <alpha>5</alpha>
      <fill_color>00CC00</fill_color>
      <fill_alpha>5</fill_alpha>
    </y>
  </grid>
  <axes>
    <x>
      <width>0</width>
      <color>FFFFFF</color>
    </x>
    <y>
      <color>006600</color>
    </y>
  </axes>
  <values>
    <x>
      <enabled>0</enabled>
	  <text_size>8</text_size>
      <skip_first>0</skip_first>
    </x>
	<y>
      <color>999999</color>
      <text_size>10</text_size>
    </y>
  </values>
  <legend>
    <enabled>0</enabled>
  </legend>
  
  <bullets>
    <hover_brightness>20</hover_brightness>
    <grow_time>1.8</grow_time>
    <grow_effect>strong</grow_effect>
    <sequenced_grow>1</sequenced_grow>
  </bullets>
  <graphs>
    <graph gid="0">
      <color>00CC33</color>
      <alpha>0</alpha>
      <balloon_text>{description}:{value}</balloon_text>
      <bullet>round_outlined</bullet>
      <bullet_size>3</bullet_size>
    </graph>
  </graphs>
  <labels>
    <label lid="0">
       <text><![CDATA[<font face="Calibri">No. of Videos</font>]]></text>
       <x>10</x>
       <y>200</y>
       <width>0</width>
       <text_size>15</text_size>
	   <fill_color>333333</fill_color>
       <rotate>1</rotate>
       <align>right</align>
    </label>
  </labels>
  <strings>
    <no_data>No Data for selected Geography/Period</no_data>
  </strings>
</settings>